{
	"name": "Nitrolaunch Transfer",
	"description": "Instance transfer support that supports everything in Nitrolaunch instances",
	"version": "v5",
	"protocol_version": 3,
	"hooks": {
		"add_instance_transfer_formats": {
			"constant": [
				{
					"id": "nitrolaunch",
					"name": "Nitrolaunch",
					"import": {
						"launch_settings": "plugin_unsupported"
					},
					"export": {
						"launch_settings": "plugin_unsupported"
					}
				}
			]
		},
		"import_instance": {
			"wasm": true
		},
		"export_instance": {
			"wasm": true
		}
	}
}