{
	"name": "Sponge",
	"description": "Install Sponge server instances",
	"protocol_version": 2,
	"hooks": {
		"on_instance_setup": {
			"executable": "${PLUGIN_DIR}/mcvm_plugin_sponge"
		},
		"add_supported_game_modifications": {
			"constant": {
				"server_types": [
					"sponge"
				]
			}
		}
	}
}