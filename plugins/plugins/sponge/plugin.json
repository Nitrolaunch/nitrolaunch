{
	"name": "Sponge",
	"description": "Install Sponge server instances",
	"version": "v5",
	"protocol_version": 3,
	"hooks": {
		"on_instance_setup": {
			"executable": "${PLUGIN_DIR}/nitro_plugin_sponge${EXE_EXTENSION}"
		},
		"add_supported_loaders": {
			"constant": [
				"sponge"
			]
		},
		"get_loader_versions": {
			"executable": "${PLUGIN_DIR}/nitro_plugin_sponge${EXE_EXTENSION}"
		}
	}
}