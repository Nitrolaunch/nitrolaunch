{
	"name": "Fabric/Quilt",
	"description": "Install Fabric and Quilt instances",
	"hooks": {
		"on_instance_setup": {
			"executable": "${PLUGIN_DIR}/mcvm_plugin_fabric_quilt"
		},
		"add_supported_game_modifications": {
			"constant": {
				"client_types": [
					"fabric",
					"quilt"
				],
				"server_types": [
					"fabric",
					"quilt"
				]
			}
		}
	}
}