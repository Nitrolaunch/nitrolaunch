{
	"name": "Fabric Quilt",
	"description": "Install Fabric and Quilt instances",
	"protocol_version": 3,
	"hooks": {
		"on_instance_setup": {
			"executable": "${PLUGIN_DIR}/nitro_plugin_fabric_quilt${EXE_EXTENSION}"
		},
		"add_supported_loaders": {
			"constant": [
				"fabric",
				"quilt"
			]
		}
	}
}